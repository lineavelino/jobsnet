{
  "openapi": "3.0.0",
  "info": {
    "description": "Conectar profissionais às oportunidades de emprego.",
    "version": "1.0.0",
    "title": "JobsNet",
    "contact": {
      "email": "aline.savelino@gmail.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "host": "localhost:5000",
  "basePath": "/",
  "tags": [
    {
      "name": "oportunidades",
      "description": "Recrutamento e seleção de profissionais nas mais diversas áreas."
    }
  ],
  "paths": {
    "/register": {
      "post": {
        "summary": "Cadastrar profissional",
        "description": "Inserção de profissionais na base de dados para conexão com as vagas disponíveis.",
        "tags": [
          "profissional"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "Aline Avelino"
                  },
                  "role": {
                    "type": "string",
                    "example": "Desenvolvedora Fullstack"
                  },
                  "birthDate": {
                    "type": "string",
                    "example": "1993-06-18T00:00:00.000Z"
                  },
                  "cpf": {
                    "type": "number",
                    "example": "32750824001"
                  },
                  "maritalStatus": {
                    "type": "string",
                    "example": "Casada"
                  },
                  "gender": {
                    "type": "string",
                    "example": "Feminino"
                  },
                  "zipCode": {
                    "type": "number",
                    "example": "20785501"
                  },
                  "address": {
                    "type": "string",
                    "example": "Rua dos Bobos"
                  },
                  "houseNumber": {
                    "type": "number",
                    "example": "0"
                  },
                  "houseComplement": {
                    "type": "string",
                    "example": "Fundos"
                  },
                  "neighborhood": {
                    "type": "string",
                    "example": "Senador Vasconcelos"
                  },
                  "city": {
                    "type": "string",
                    "example": "Rio de Janeiro"
                  },
                  "uf": {
                    "type": "string",
                    "example": "RJ"
                  },
                  "telephone1": {
                    "type": "number",
                    "example": "21979683375"
                  },
                  "email": {
                    "type": "string",
                    "example": "aline.savelino@gmail.com"
                  },
                  "linkedin": {
                    "type": "string",
                    "example": "https://www.linkedin.com/in/alineavelinorocha/"
                  },
                  "github": {
                    "type": "string",
                    "example": "https://www.github.com/lineavelino"
                  },
                  "facebook": {
                    "type": "string",
                    "example": "facebook.com/"
                  },
                  "twitter": {
                    "type": "string",
                    "example": "https://www.twitter.com/lineavelino"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}